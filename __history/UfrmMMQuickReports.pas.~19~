    QRGroup34: TQRGroup;
    QRDBText725: TQRDBText;
    QRLabel57: TQRLabel;
    procedure FormCreate(Sender: TObject);
    procedure DivideValue(sender: TObject; var Value: String);
    procedure LabelPrefix(sender: TObject; var Value: String);
    procedure spMMDealNoteViewAfterScroll(DataSet: TDataSet);
    procedure grpClientBeforePrint(Sender: TQRCustomBand;
      var PrintBand: Boolean);
    procedure QRGroup15BeforePrint(Sender: TQRCustomBand;
      var PrintBand: Boolean);
    procedure QRConsolidatedReportCompAddReports(Sender: TObject);
  private
    { Private declarations }
    DivideValues: String;
  public
    { Public declarations }
  end;

var
  frmMMQuickReports: TfrmMMQuickReports;

implementation

uses UdtmMain, UfrmMain;

{$R *.dfm}

procedure TfrmMMQuickReports.FormCreate(Sender: TObject);
begin
    if Assigned(dtmMain) then begin
        tblCompany.Open;
        tblCompany.First;
    end;
end;

procedure TfrmMMQuickReports.grpClientBeforePrint(Sender: TQRCustomBand;
  var PrintBand: Boolean);
begin
    PrintBand := not spConsolidatedReportIsClient.Value;
end;

// TAU
procedure TfrmMMQuickReports.DivideValue(sender: TObject;
  var Value: String);
begin
    DivideValues := frmMain.cboDivideValues.Text;

    try
        if DivideValues = 'Thousands' then
            Value := FormatFloat(',#0.00', StrToFloat(StringReplace(Value,',','',[rfReplaceAll, rfIgnoreCase])) / 1000)
        else if DivideValues = 'Millions' then
            Value := FormatFloat(',#0.00', StrToFloat(StringReplace(Value,',','',[rfReplaceAll, rfIgnoreCase])) / 1000000)
        else if DivideValues = 'Billions' then
            Value := FormatFloat(',#0.00', StrToFloat(StringReplace(Value,',','',[rfReplaceAll, rfIgnoreCase])) / 1000000000)
        else if DivideValues = 'Trillions' then
            Value := FormatFloat(',#0.00', StrToFloat(StringReplace(Value,',','',[rfReplaceAll, rfIgnoreCase])) / 1000000000000);
    except
    end;
end;

procedure TfrmMMQuickReports.LabelPrefix(sender: TObject;
  var Value: String);
begin
    DivideValues := frmMain.cboDivideValues.Text;

    if DivideValues = 'Thousands' then
        Value := Value + ':K'
    else if DivideValues = 'Millions' then
        Value := Value + ':M'
    else if DivideValues = 'Billions' then
        Value := Value + ':B'
    else if DivideValues = 'Trillions' then
        Value := Value + ':T';
end;

procedure TfrmMMQuickReports.QRConsolidatedReportCompAddReports(
  Sender: TObject);
begin
QRConsolidatedReportComp.Reports.Add(QRConsolidatedReportSummary);
QRConsolidatedReportComp.Reports.Add(QRConsolidatedReport);

end;

procedure TfrmMMQuickReports.QRGroup15BeforePrint(Sender: TQRCustomBand;
  var PrintBand: Boolean);
begin
    if spConsolidatedReportInvestmentTypeName.Value = 'Equities' then
        QRDBText638.Mask := '#,##0.0000'
    else
        QRDBText638.Mask := '#,##0.00';
end;

procedure TfrmMMQuickReports.spMMDealNoteViewAfterScroll(
  DataSet: TDataSet);
begin
    { Hide or show VAT }
    if spMMDealNoteViewVAT.Value = 0 then begin
        lblVATLabel.Font.Color := clWhite;
        lblVATData.Font.Color := clWhite;
    end else begin
        lblVATLabel.Font.Color := clWindowText;
        lblVATData.Font.Color := clWindowText;
    end;
end;


end.
