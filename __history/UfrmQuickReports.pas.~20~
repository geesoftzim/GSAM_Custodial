end;

procedure TfrmQuickReports.DivideValue(sender: TObject;
  var Value: String);
begin
    DivideValues := frmMain.cboDivideValues.Text;

    try
        if DivideValues = 'Thousands' then
            Value := FormatFloat(',#0.00', dtmMain.Decomma(Value) / 1000)
        else if DivideValues = 'Millions' then
            Value := FormatFloat(',#0.00', dtmMain.Decomma(Value) / 1000000)
        else if DivideValues = 'Billions' then
            Value := FormatFloat(',#0.00', dtmMain.Decomma(Value) / 1000000000)
        else if DivideValues = 'Trillions' then
            Value := FormatFloat(',#0.00', dtmMain.Decomma(Value) / 1000000000000);
    except
    end;
end;

procedure TfrmQuickReports.LabelPrefix(sender: TObject;
  var Value: String);
begin
    DivideValues := frmMain.cboDivideValues.Text;

    if DivideValues = 'Thousands' then
        Value := Value + ':K'
    else if DivideValues = 'Millions' then
        Value := Value + ':M'
    else if DivideValues = 'Billions' then
        Value := Value + ':B'
    else if DivideValues = 'Trillions' then
        Value := Value + ':T';
end;

procedure TfrmQuickReports.QRStatementAfterPrint(Sender: TObject);
begin
    if dtmMain.ChargeStatement then
    begin
        with spStatementPrintCharge do
        begin
            Parameters.ParamByName('@AccountID').Value := SpStatementTransactionDetailsAccountID.Value;
            Parameters.ParamByName('@ValueDate').Value := DateOf(Today);
            Prepared := True;
            ExecProc;
        end;
    end;
    dtmMain.ChargeStatement := False;
end;

procedure TfrmQuickReports.QRDBText711Print(sender: TObject;
  var Value: String);
begin
  QRExpr149.Reset;
end;

end.
