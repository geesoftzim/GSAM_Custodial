var
  frmQuickReports: TfrmQuickReports;

implementation

uses UdtmMain, UfrmMain;

{$R *.dfm}

procedure TfrmQuickReports.FormCreate(Sender: TObject);
var
    i: Integer;
begin
    if Assigned(dtmMain) then begin
        tblCompany.Open;
        tblCompany.First;
    end;
    for i := 0 to self.ComponentCount - 1 do
    begin
        if Self.Components[i] is TQRDBImage then
        begin
            if TQRDBImage(Self.Components[i]).DataField = 'Logo' then
            begin                
                TQRDBImage(Self.Components[i]).Center := tblCompanyCenterLogo.Value;
            end;
        end;
    end;

end;

procedure TfrmQuickReports.QRRtgsFormAfterPrint(Sender: TObject);
begin
    with spTransactionSchedulePrint do begin
        Parameters.ParamByName('@TransactionScheduleID').Value := spTransactionScheduleViewID.Value;
        Prepared := true;
        ExecProc;
    end;
//    spTransactionTicketDetailsPrintCount.Value := spTransactionTicketDetailsPrintCount.Value + 1;
end;

procedure TfrmQuickReports.QRBand196BeforePrint(Sender: TQRCustomBand;
  var PrintBand: Boolean);
begin
    // Spawn a new page to have NMI Rebate totals on their own
    Sender.ParentReport.NewPage;
end;

